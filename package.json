    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "extraResources": [
        {
          "from": "./node_modules/node-adodb/lib/adodb.js",
          "to": "adodb.js"
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "createDesktopShortcut": true
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "publish": [
      {
        "provider": "generic",
