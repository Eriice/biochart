    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist": "ng build --base-href ./ && tsc --p electron && electron-builder --win --x64",
    "electron": "ng build --base-href ./ && tsc --p electron && electron electron/dist/main.js"
  },
  "main": "electron/dist/main.js",
  "build": {
    "directories": {
      "output": "builder"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
